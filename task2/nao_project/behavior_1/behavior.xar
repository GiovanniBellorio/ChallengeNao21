                  <![CDATA[import math
import motion
import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.yawAngle = 0
        self.play = False

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_yawAngle(self, yawAngle):
        #self.logger.info(yawAngle)
        # posizioni manuali
        if (yawAngle == "dx"):
            self.yawAngle += -0.1
        elif (yawAngle == "sx"):
            self.yawAngle += 0.1
        elif (yawAngle == "res"):
            self.yawAngle = 0.0
        elif (yawAngle == "giu"):
            pass
        elif (yawAngle == "su"):
            pass
        elif (yawAngle == "pl"):
            self.play = True
        else:
            # giroscopio
            tmp = -(int(yawAngle))
            if (tmp > -20 and tmp < 20):
                self.yawAngle = 0.0 #0.1
            if (tmp < -20):
                self.yawAngle = -0.88 #-0.78
            if (tmp > 20):
                self.yawAngle = 0.88 #0.98

        self.onInput_onStart()

    def onInput_onStart(self):
        motionProxy = ALProxy("ALMotion")
        angles = motionProxy.getAngles('Body',False)
        bodyNames = motionProxy.getBodyNames("Body")
        #self.logger.info(bodyNames)

        motionProxy.setStiffnesses("Head", 1.0)
        names  = ["HeadYaw", "HeadPitch"]
        angles = [self.yawAngle, 0.0]
        fractionMaxSpeed  = 0.1
        motionProxy.setAngles(names, angles, fractionMaxSpeed)

        time.sleep(3)
        motionProxy.setStiffnesses("Head", 0.0)
        self.logger.info(self.yawAngle)

        # output opera
        if (self.play and self.yawAngle == -0.88):
            self.opera(1)
        elif (self.play and self.yawAngle == 0.0):
            self.opera(2)
        elif (self.play and self.yawAngle == 0.88):
            self.opera(3)
        else:
            self.onStopped()
        self.play = False


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
